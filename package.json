{
  "name": "tsxt",
  "version": "0.0.1",
  "description": "JSX based template engine with types",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist-lib && rm -rf ./dist-examples && rm -rf ./dist-tests",
    "build": "npm run clean && npm run build:plugin && npm run build:examples && npm run build:tests",
    "build:plugin": "tsc -p ./tsconfig.json",
    "build:examples": "babel --config-file ./examples/babel.config.json --extensions .tsx,.ts --out-dir ./dist-examples ./examples",
    "build:tests": "babel --config-file ./tests/babel.config.json --extensions .tsx,.ts --out-dir ./dist-tests ./tests"
  },
  "keywords": [
    "JSX",
    "TSX",
    "TypeScript",
    "template"
  ],
  "author": "Vladislav Lebedev",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/helper-plugin-utils": "^7.14.5",
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/traverse": "^7.16.3",
    "@babel/types": "^7.16.0",
    "@types/babel__core": "^7.1.16",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/babel__traverse": "^7.14.2",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "@types/node": "^16.11.7"
  }
}
