{
  "presets": [
    ["@babel/preset-env", { "targets": { "node": "current" } }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-syntax-typescript",
    [
      "../dist-lib/src/index.js",
      {
        "indentType": "space",
        "indentSize": 4,
        "codeblockStart": "{",
        "codeblockEnd": "}"
      }
    ]
  ]
}
