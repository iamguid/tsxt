{
  "name": "tsxt-core",
  "version": "1.5.1",
  "description": "JSX based template engine with types",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "homepage": "https://github.com/iamguid/tsxt",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/iamguid/tsxt"
  },
  "scripts": {
    "test": "yarn jest",
    "clean": "rm -rf ./dist && rm -rf ./dist-tests",
    "build:all": "npm run clean && npm run build:plugin && npm run build:tests",
    "build:plugin": "tsc -p ./tsconfig.json",
    "build:tests": "babel --config-file ./babel.config.json --extensions .tsx,.ts --out-dir ./dist-tests ./tests"
  },
  "keywords": [
    "JSX",
    "TSX",
    "TypeScript",
    "template",
    "template engine"
  ],
  "author": "Vladislav Lebedev <iam.guid@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "7.21.4",
    "@babel/core": "7.21.4",
    "@babel/generator": "^7.21.4",
    "@babel/helper-plugin-utils": "7.20.2",
    "@babel/plugin-syntax-jsx": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-typescript": "7.21.4",
    "@babel/traverse": "7.21.4",
    "@babel/types": "7.21.4",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/template": "7.20.7",
    "@types/babel__code-frame": "7.0.3",
    "@types/babel__core": "7.20.0",
    "@types/babel__helper-plugin-utils": "7.10.0",
    "@types/babel__traverse": "7.18.3",
    "@types/jest": "^29.5.1",
    "@types/node": "18.15.13",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.5.0",
    "prettier": "2.8.7",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  }
}
