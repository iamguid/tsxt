{
  "name": "tsxt-cli",
  "version": "0.0.1",
  "description": "CLI for tsxt template engine",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "homepage": "https://github.com/iamguid/tsxt",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamguid/tsxt"
  },
  "bin": "./dist/bin/index.js",
