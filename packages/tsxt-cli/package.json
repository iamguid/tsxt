  "dependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-react": "7.18.6",
    "jest": "29.5.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tsxt-core": "0.0.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/jest": "29.5.1"
  }
}
